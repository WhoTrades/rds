{
    "name": "whotrades/rds",
    "type": "library",
    "description": "WhoTrades release deployment service",
    "repositories": [
        {
            "type": "composer",
            "url": "https://artifact.finam.ru/artifactory/api/composer/packagist-org"
        },
        {
            "packagist": false
        }
    ],
    "autoload": {
        "psr-0": {
            "Whotrades_": "../../code-standard/Standards/"
        }
    },
    "require": {
        "fxp/composer-asset-plugin": "~1.0",
        "react/zmq": "~0.3",
        "php-amqplib/php-amqplib": "~2.6",
        "whotrades/phpcs-stash": "0.0.13",
        "escapestudios/symfony2-coding-standard": "2.9.0",
        "yiisoft/yii2": "~2.0",
        "yiisoft/yii2-bootstrap": "~2.0",
        "yiisoft/yii2-gii": "~2.0.0",
        "php": ">=7.0",
        "yiisoft/yii2-swiftmailer": "~2.0",
        "kartik-v/yii2-widgets": "~3.4",
        "sentry/sentry": "1.1.0"
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
