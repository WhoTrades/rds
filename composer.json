        "whotrades/phpcs-stash": "0.0.12",
        "escapestudios/symfony2-coding-standard": "2.9.0",
        "yiisoft/yii": "1.1.17",
        "crisu83/yiistrap": "2.0.3",
        "php": ">=5.6.0",
        "sentry/sentry": "1.1.0"
    }
