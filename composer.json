{
    "name": "whotrades/rds",
    "type": "service",
    "author": "Artem Naumenko (entsupml@gmail.com)",
    "description": "WhoTrades release deployment service",
    "autoload": {
        "psr-4": {
            "whotrades\\rds\\": "src/"
        }
    },
    "require": {
        "kartik-v/yii2-grid": "~3.1",
        "fxp/composer-asset-plugin": "~1.0",
        "react/zmq": "~0.3",
        "php-amqplib/php-amqplib": "~2.6",
        "whotrades/rds-system": "dev-master",
        "yiisoft/yii2": "2.0.13",
        "yiisoft/yii2-bootstrap": "~2.0",
        "yiisoft/yii2-gii": "~2.0.0",
        "php": ">=7.0",
        "yiisoft/yii2-swiftmailer": "~2.0",
        "sentry/sentry": "1.6.2",
        "dektrium/yii2-user": "0.9.12",
        "conquer/codemirror": "~2.0",
        "kartik-v/yii2-widget-select2": "2.0.9",
        "codemix/yii2-streamlog": "1.2.1",
        "bazilio/yii2-stubs-generator": "~1",
        "mito/yii2-sentry": "~1.0.0"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "2.0.9"
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
